    {
        "client_msg_id": "30a38a32-5258-4039-89ed-da39a17cdb94",
        "type": "message",
        "text": "<@UE1JQM9HQ> <@UNGNT3BKR> What name do you guys use for this pattern of an automatic mapping between UI, model, and storage?",
        "user": "ULM3U6275",
        "ts": "1569603303.051800",
        "team": "T5TCAFTA9",
        "user_team": "T5TCAFTA9",
