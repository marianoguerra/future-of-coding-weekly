                                "text": "translate_emulated",
                                "style": {
                                    "code": true
                                }
                            },
                            {
                                "type": "text",
                                "text": " rather than just "
                            },
                            {
                                "type": "text",
                                "text": "translate",
                                "style": {
                                    "code": true
                                }
                            },
                            {
                                "type": "text",
